<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Herrera_Dilan_Examen_P2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Herrera_Dilan_Examen_P2"
    Title="Herrera_Dilan_Examen_P2">

    <TabBar>
        <Tab Title="MainPage" Icon="dotnet_bot.png">
            <ShellContent
            Title="MainPage"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        </Tab>

        <Tab Title="PaginaRecargas" Icon="dotnet_bot.png">
            <ShellContent
            Title="PaginaRecargas"
            ContentTemplate="{DataTemplate local:PaginaRecargas}"
            Route="PaginaRecargas" />
        </Tab>

        <Tab Title="PaginaNombres" Icon="dotnet_bot.png">
            <ShellContent
            Title="PaginaNombres"
            ContentTemplate="{DataTemplate local:PaginaNombres}"
            Route="PaginaNombres" />
        </Tab>
    </TabBar>

</Shell>
